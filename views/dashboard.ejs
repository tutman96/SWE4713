<%- include('partials/header') %>
<span class="title"><%= title %></span>
<br><br>
<div class="row" style="margin: 0 auto; max-width: 1200px">
	<div class="col-sm-6">
		<div class="panel panel-default">
			<div class="panel-heading">
				Asset Turnover Ratios
			</div>
			<table class="table table-hover table-condensed">
				<thead>
					<tr>
						<th class="col-sm-2"></th>
						<% for (var d of data) { %>
							<th><%=dateFormat(new Date(d.Year, d.Month - 1), 'MMM YYYY')%></th>
						<% } %>
					</tr>
				</thead>
				<tr>
					<th class="col-sm-2">Fixed Asset Turnover</th>
					<% for (var d of data) { %>
						<td><%=d.FATO.toFixed(2)%> <i class="glyphicon glyphicon-ok-circle text-success"></i></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Total Asset Turnover</th>
					<% for (var d of data) { %>
						<td><%=d.TAT.toFixed(2)%> <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Inventory Turnover</th>
					<% for (var d of data) { %>
						<td>N/A</td>
					<% } %>
				</tr>
			</table>
		</div>
		
		<div class="panel panel-default">
			<div class="panel-heading">
				Debt Ratios
			</div>
			<table class="table table-hover table-condensed">
				<thead>
					<tr>
						<th class="col-sm-2"></th>
						<% for (var d of data) { %>
							<th><%=dateFormat(new Date(d.Year, d.Month - 1), 'MMM YYYY')%></th>
						<% } %>
					</tr>
				</thead>
				<tr>
					<th class="col-sm-2">Debt Ratio</th>
					<% for (var d of data) { %>
						<td><%=(d.DR * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Times Interest Earned</th>
					<% for (var d of data) { %>
						<td>N/A</td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">EBITDA Coverage</th>
					<% for (var d of data) { %>
						<td>N/A</td>
					<% } %>
				</tr>
			</table>
		</div>
		
		<div class="panel panel-default">
			<div class="panel-heading">
				Profit Margins
			</div>
			<table class="table table-hover table-condensed">
				<thead>
					<tr>
						<th class="col-sm-2"></th>
						<% for (var d of data) { %>
							<th><%=dateFormat(new Date(d.Year, d.Month - 1), 'MMM YYYY')%></th>
						<% } %>
					</tr>
				</thead>
				<tr>
					<th class="col-sm-2">Profit Margin</th>
					<% for (var d of data) { %>
						<td><%=(d.PM * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Operating Profit Margin</th>
					<% for (var d of data) { %>
						<td><%=(d.OPM * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Gross Profit Margin</th>
					<% for (var d of data) { %>
						<td><%=(d.GPM * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
			</table>
		</div>
	</div>
	<div class="col-sm-6">
		
		<div class="panel panel-default">
			<div class="panel-heading">
				Earning Power
			</div>
			<table class="table table-hover table-condensed">
				<thead>
					<tr>
						<th class="col-sm-2"></th>
						<% for (var d of data) { %>
							<th><%=dateFormat(new Date(d.Year, d.Month - 1), 'MMM YYYY')%></th>
						<% } %>
					</tr>
				</thead>
				<tr>
					<th class="col-sm-2">Basic Earning Power</th>
					<% for (var d of data) { %>
						<td><%=(d.BEP * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
			</table>
		</div>
		
		<div class="panel panel-default">
			<div class="panel-heading">
				Returns
			</div>
			<table class="table table-hover table-condensed">
				<thead>
					<tr>
						<th class="col-sm-2"></th>
						<% for (var d of data) { %>
							<th><%=dateFormat(new Date(d.Year, d.Month - 1), 'MMM YYYY')%></th>
						<% } %>
					</tr>
				</thead>
				<tr>
					<th class="col-sm-2">Return on Investment</th>
					<% for (var d of data) { %>
						<td><%=(d.ROA * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Return on Equity</th>
					<% for (var d of data) { %>
						<td><%=(d.ROE * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
			</table>
		</div>
		
		<div class="panel panel-default">
			<div class="panel-heading">
				Liquidity Ratios
			</div>
			<table class="table table-hover table-condensed">
				<thead>
					<tr>
						<th class="col-sm-2"></th>
						<% for (var d of data) { %>
							<th><%=dateFormat(new Date(d.Year, d.Month - 1), 'MMM YYYY')%></th>
						<% } %>
					</tr>
				</thead>
				<tr>
					<th class="col-sm-2">Current Ratio</th>
					<% for (var d of data) { %>
						<td><%=(d.CR * 100).toFixed(1)%>% <i class="glyphicon glyphicon-ok-circle text-success"></td>
					<% } %>
				</tr>
				<tr>
					<th class="col-sm-2">Quick Ratio</th>
					<% for (var d of data) { %>
						<td>N/A</td>
					<% } %>
				</tr>
			</table>
		</div>
	</div>
</div>

<%- include('partials/footer') %>